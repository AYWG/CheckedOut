extends layout.pug

block append head-content
  link(href='css/manage_lists.css', rel='stylesheet', type='text/css')
                  
block content
  // SEARCH RESULTS
  .Manage
    form
      h5 Your Lists
      ul.list-group
        for list in user_lists
          li.list-group-item
            a.viewList(href='#') #{list}

block append footer-scripts
  script(src='js/get_list.js', type='text/javascript')            
  script(src='js/list.js', type='text/javascript')
		
      
        
